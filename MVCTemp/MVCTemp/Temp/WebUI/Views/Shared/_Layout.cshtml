<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="renderer" content="webkit">
    <title>@ViewBag.Title</title>
    <link href="~/Content/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/css/custom/base.css" rel="stylesheet" />
    <link href="~/Content/css/custom/common.css" rel="stylesheet" />
    <link href="~/Content/css/topleft.css" rel="stylesheet" />
    <link href="~/Content/css/public.css" rel="stylesheet" />
    <link href="~/Content/js/layer/skin/layer.css" rel="stylesheet" />

    @RenderSection("headSection", false)
</head>
<body>

    @{Html.RenderPartial("Top");}

    <!--left-->
    <div id="left" class="main-left">
        @{Html.RenderPartial("Menu");}
    </div>
    <div style="height: 90px"></div>
    <div class="main-right">
        @RenderBody()
    </div>
    <script src="~/Content/js/sea.js"></script>
    <script src="~/Content/js/jquery-1.7.2.min.js"></script>
    <script type="text/javascript">
        seajs.config({
            base: "/Content/js",
            alias: {
                "jquery.easing": "jquery.easing.1.3.js",
                "menu": "login/menu.js",
                "WdatePicker": "My97DatePicker/WdatePicker.js",
                "bootstrap": "bootstrap.min.js",
                "jqueryTools": "jqueryTools.js",
                "json2": "json2.js",
                "layer": "layer/layer.js",
                "sso": "sso.js",
                "moment": "moment.min.js",
                "commonFunction": "commonFunctions.js",
                "outLogin": "login/outLogin.js",
                "jsRender": "jsrender.min.js",
                "tools-detail": "tools-detail.js",
                "html5Validate": "jquery-html5Validate-min.js",
                "jquery.cookie": "jquery.cookie.js",
                "BrowserTips": "BrowserTips",
                "jsRenderExtPagination": "jsRenderExtPagination-1.0.4.min.js"
            },
            preload: [$ ? "" : "/Content/js/jquery-1.7.2.min.js"]
        });
        seajs.use(["jquery.easing", "menu", "WdatePicker", "commonFunction", "bootstrap", "jqueryTools", "json2", "jsRender", "layer", "moment", "outLogin", "jquery.cookie", "BrowserTips", "sso"]);
    </script>

    <script type="text/javascript">
        $.ajaxSetup({
            statusCode: {
                309: function (data) {
                    window.location.href = "/Login/Index";
                }
            },
            cache: false,
            error: function (msg) {

            }
        });
    </script>

    @RenderSection("scripts", required: false)
</body>
</html>
